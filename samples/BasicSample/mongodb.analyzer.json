{
    // Enables builders variables tracking and composition, enabled by default
    "EnableVariableTracking": true,

    // Appends the driver version to MQL expression
    "OutputDriverVersion": false,

    // Outputs internal analyzer exceptions
    "OutputInternalExceptions": true,

    // Outputs internal logs to a file
    "OutputInternalLogsToFile": false,

    // Logs file, to be used in conjunction with OutputInternalLogsToFile setting
    "LogFileName": "c:\\logs\\analyzerlogs.txt",

    // LINQ analysis verbosity, defaults to Medium
    // "LinqAnalysisVerbosity": "All",

    // POCO analysis verbosity, defaults to Medium
    "PocoAnalysisVerbosity": "All"
}
